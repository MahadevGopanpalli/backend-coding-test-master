{
    "env": {
      "node": true,
      "jest": true
    },
    "globals": {
        "before": "readonly"
      },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
    //   "no-console": "warn",
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
  
